.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 20px;
}

.button-group {
  display: flex;
  justify-content: space-between;
  width: 100%;
}
.button-group button {
  /* Add styles for the buttons here, e.g.: */
  padding: 10px 20px;
  font-size: 16px;
  cursor: pointer;
  border: none;
  color: white;
  background-color: #1AAD19;
  border-radius: 5px;
}

.free-team {
  /* Add specific styles for the "Free Team" button here, if needed */
  background-color: #3B75E3;
}